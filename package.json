{
  "name": "echec-react",
  "version": "1.0.0",
  "description": "Echec.ca version in io.js, ES6, and react with a flux architecture.",
  "main": "app.js",
  "scripts": {
    "start": "watchify -o static/bundle.js -v -d js/**/*",
    "test": "jest",
    "build": "NODE_ENV=production browserify . | uglifyjs -cm > bundle.min.js"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "browserify": "^9.0.3",
    "co-views": "^0.3.0",
    "envify": "^3.2.0",
    "flux": "^2.0.1",
    "gulp": "^3.8.11",
    "jade": "^1.9.2",
    "jquery": "^2.1.3",
    "koa-json": "^1.1.1",
    "koa-less": "0.0.3",
    "koa-router": "^4.2.0",
    "koa-static": "^1.4.9",
    "material-ui": "^0.7.3",
    "react": "^0.13.1",
    "react-router": "^0.13.2",
    "react-tap-event-plugin": "^0.1.6",
    "react-tools": "^0.12.2",
    "reactify": "^1.0.0",
    "underscore": "^1.8.3",
    "watchify": "^2.4.0"
  },
  "browserify": {
    "transform": [
      "envify",
      "babelify"
    ]
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/node_modules/babel-jest",
    "testFileExtensions": [
      "es6",
      "js"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "es6"
    ],
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react"
    ]
  },
  "devDependencies": {
    "babel-jest": "^5.0.1",
    "babelify": "^5.0.3",
    "jest-cli": "^0.4.0"
  }
}
